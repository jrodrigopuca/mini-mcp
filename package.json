{
  "name": "mini-mcp",
  "version": "1.0.0",
  "description": "MCP server for natural language CSV/data analysis with DuckDB",
  "main": "dist/index.js",
  "bin": {
    "mini-mcp": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "check": "biome check --write src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://jrodrigopuca@github.com/jrodrigopuca/mini-mcp.git"
  },
  "keywords": [
    "mcp",
    "csv",
    "duckdb",
    "data-analysis",
    "natural-language"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jrodrigopuca/mini-mcp/issues"
  },
  "homepage": "https://github.com/jrodrigopuca/mini-mcp#readme",
  "dependencies": {
    "@duckdb/node-api": "^1.4.4-r.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "csv-parse": "^6.1.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.3",
    "@types/node": "^22.13.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
